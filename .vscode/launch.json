  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Chrome",
      "port": 8080,
      "request": "attach",
      "type": "pwa-chrome",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "1942 Launch",
      "url": "http://192.168.1.2:8080",
      "webRoot": "${workspaceFolder}",
      "disableNetworkCache" : true
    }
  ]
}